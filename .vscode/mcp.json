{
  "servers": {
    "hyper-mcp": {
      "type": "stdio",
      "command": "hyper-mcp",
      "args": ["--config-file", ".ai/hyper-mcp.yaml"]
    },
    "mycommandmcp": {
      "type": "stdio",
      "command": "mycommandmcp",
      "args": ["--config", ".ai/mycommand-mcp.yaml"]
    },
    "serena": {
      "type": "stdio",
      "command": "pkgx",
      "args": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "./"
      ]
    },
    "remembrances": {
      "type": "stdio",
      "command": "./build/remembrances-mcp",
      "args": ["--config", "./config.sample.gguf.yaml"]
    }
  },
  "inputs": []
}
